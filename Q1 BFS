#include <iostream>
#include <queue>
#include <vector>
using namespace std;

int main() {
    int n, e;
    cin >> n >> e;
    vector<vector<int>> g(n);

    for (int i = 0; i < e; i++) {
        int u, v;
        cin >> u >> v;
        g[u].push_back(v);
        g[v].push_back(u);
    }

    vector<int> visited(n, 0);
    queue<int> q;

    int start;
    cin >> start;
    visited[start] = 1;
    q.push(start);

    while (!q.empty()) {
        int node = q.front();
        q.pop();
        cout << node << " ";

        for (int x : g[node]) {
            if (!visited[x]) {
                visited[x] = 1;
                q.push(x);
            }
        }
    }
}
