#include <iostream>
#include <vector>
#include <queue>
using namespace std;

int main() {
    int n, e;
    cin >> n >> e;

    vector<vector<pair<int,int>>> g(n);
    for (int i = 0; i < e; i++) {
        int u, v, w;
        cin >> u >> v >> w;
        g[u].push_back({v, w});
        g[v].push_back({u, w});
    }

    vector<int> vis(n, 0);
    priority_queue<
        pair<int,int>, 
        vector<pair<int,int>>, 
        greater<pair<int,int>>
    > pq;

    pq.push({0, 0}); // cost, node
    int total = 0;

    while (!pq.empty()) {
        auto [w, node] = pq.top();
        pq.pop();

        if (vis[node]) continue;
        vis[node] = 1;
        total += w;

        for (auto &p : g[node]) {
            pq.push({p.second, p.first});
        }
    }

    cout << "Total Cost: " << total;
}
